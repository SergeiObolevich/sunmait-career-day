{
  "dependencies": {
    "@material-ui/core": "^3.3.2",
    "@material-ui/icons": "^3.0.1",
    "@types/history": "^4.7.2",
    "@types/query-string": "^6.1.1",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "@types/react-redux": "^6.0.9",
    "@types/react-router": "^4.0.32",
    "@types/react-router-dom": "^4.3.1",
    "@types/redux-promise-middleware": "0.0.11",
    "axios": "^0.18.0",
    "material-ui-pickers": "^1.0.1",
    "moment": "^2.22.2",
    "query-string": "^6.2.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.5",
    "redux-promise-middleware": "^5.1.1",
    "redux-thunk": "^2.3.0"
  },
  "engines": {
    "node": ">=4.5.0"
  },
  "scripts": {
    "build": "webpack --config webpack/dev.config.js --watch --progress",
    "build-prod": "NODE_ENV=production node_modules/webpack/bin/webpack.js --config webpack/prod.config.js",
    "test": "jest --coverage",
    "e2e-test": "cd ../server && npm run seed:undo && npm run seed && cd ../client && ./node_modules/.bin/cypress open"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.1.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@types/jquery": "^3.3.22",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "compression-webpack-plugin": "^2.0.0",
    "css-loader": "^1.0.0",
    "cssnano": "^4.1.7",
    "cypress": "^3.1.0",
    "cypress-cucumber-preprocessor": "^1.6.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.4",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "sugarss": "^2.0.0",
    "terser-webpack-plugin": "^1.1.0",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslint-loader": "^3.5.4",
    "typescript": "^3.1.3",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2",
    "webpack-merge": "^4.1.4"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.js$": "babel-jest"
    },
    "modulePaths": [
      "./app"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ],
    "coveragePathIgnorePatterns": [
      "app/index.tsx",
      "app/redux/store.ts"
    ],
    "clearMocks": true,
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/__mocks__/file-mock.js",
      "\\.(css|less)$": "<rootDir>/tests/__mocks__/style-mock.js"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "app/**/*.{ts}"
    ],
    "coverageThreshold": {
      "./app/redux/**/*.ts": {
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
      },
      "./app/components/helper/*.ts": {
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
      }
    },
    "testMatch": [
      "**/tests/**/*test.js?(x)"
    ]
  }
}
